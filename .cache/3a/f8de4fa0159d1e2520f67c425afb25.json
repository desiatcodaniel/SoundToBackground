n    for (let i = inputShape.length - 1; i >= 0; i--) {\r\n      if (inputShape[i] === outputShape[i]) {\r\n        reps[i] = 1;\r\n      } else if (inputShape[i] !== 1) {\r\n        throw new Error(`broadcastTo(): [${\r\n            inputShape}] cannot be broadcast to [${outputShape}].`);\r\n      }\r\n    }\r\n    const axes: number[] = [];\r\n    for (let i = 0; i < reps.length; i++) {\r\n      if (reps[i] > 1) {\r\n        axes.push(i);\r\n      }\r\n    }\r\n\r\n    return {x: () => sum(dy, axes, true /* keepDims */)};\r\n  }\r\n};\r\n"},"lineCount":null}},"error":null,"hash":"06b2c39efecc79a08fe7065733cb0fbd","cacheData":{"env":{}}}